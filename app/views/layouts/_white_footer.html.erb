<section id="white-footer">
  <div class="container">
    <div class="row-fluid" id="info-footer">
      <div class="span4" id="instagram-wrapper">
        <div class="title-divider">
          <h3>
            <style>.ig-b- { 
              display: inline-block; }
              .ig-b- img { visibility: hidden; }
              .ig-b-:hover { background-position: 0 -60px; } .ig-b-:active { background-position: 0 -120px; }
              .ig-b-v-24 { width: 137px; height: 24px; background: url(//badges.instagram.com/static/images/ig-badge-view-sprite-24.png) no-repeat 0 0; }
                @media only screen and (-webkit-min-device-pixel-ratio: 2), only screen and (min--moz-device-pixel-ratio: 2), only screen and (-o-min-device-pixel-ratio: 2 / 1), only screen and (min-device-pixel-ratio: 2), only screen and (min-resolution: 192dpi), only screen and (min-resolution: 2dppx) {
              .ig-b-v-24 { background-image: url(//badges.instagram.com/static/images/ig-badge-view-sprite-24@2x.png); background-size: 160px 178px; } }
            </style>
            <a href="http://instagram.com/jcocaram?ref=badge" target="_blank" class="ig-b- ig-b-v-24">
              <img src="//badges.instagram.com/static/images/ig-badge-view-24.png" alt="Instagram" />
            </a>
          </h3>
        </div>
        <div class="row-fluid">
          <div class="span12">
            <%= render "layouts/instagram" %>
          </div>
        </div>
      </div>
      <div class="span4">
        <div class="title-divider">
          <h3><i class="icon-twitter"></i> Twitter</h3>
          <a class="twitter-timeline" href="https://twitter.com/jcocaramos" data-widget-id="312628047968866304">Tweets by @jcocaramos</a>
          <script>
            !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");
          </script>
        </div>
      </div>
      <div class="span4">
        <div class="title-divider">
          <h3><i class="icon-user"></i> Contact Me</h3>
        </div>
        <div class="row-fluid">
          <div class="span12">
            <%= render "layouts/contact_me" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>